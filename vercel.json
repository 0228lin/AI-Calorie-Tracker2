{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/gemini.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "functions": {
    "api/gemini.js": {
      "runtime": "node18.x"
    }
  }
}
